<?page title="Asignar Taxi" contentType="text/html;charset=UTF-8"?>
<zk>
	<window id="winAsignarTaxi" title="Asignar Taxi" border="normal"
		mode="modal" width="400px" height="100px" sizable="false" closable="true">
		<borderlayout>
			<center>
				<grid width="300">
					<columns>
						<column></column>
						<column></column>
					</columns>
					<rows>
						<row>
							<label value="Nro. Pedido Pendiente:" />
							<textbox name="pedidoPendiente" />
						</row>
						<row>
							<label value="Id de Taxi:" />
							<textbox name="idTaxi" />
						</row>
					</rows>

				</grid>
			</center>
			<east>
				<vbox align="center">
					<button label="Aceptar" onClick="" />
					<button label="Cancelar" onClick="winAsignarTaxi.detach()" />
				</vbox>
			</east>
		</borderlayout>

	</window>
</zk>
