<?page title="Alta Conductor" contentType="text/html;charset=UTF-8"?>
<zk>
	<window id="winAltaConductor" title="Alta Conductor" border="normal"
		mode="modal" width="420px" height="240px" sizable="false" closable="true">
		<borderlayout>
			<center>
				<grid width="300">
					<columns>
						<column></column>
						<column></column>
					</columns>
					<rows>
						<row>
							<label value="Nombre y Apellido:" />
							<textbox id="nombreApellido" />
						</row>
						<row>
							<label value="Tipo de Documento:" />
							<combobox ><comboitem label="DNI"/><comboitem label="LE"/></combobox>
						</row>
						<row>
							<label value="Número" />
							<textbox id="numDoc"/>
						</row>
						<row>
							<label value="Dirección:" />
							<textbox id="direccion" />
						</row>
						<row>
							<label value="Localidad:" />
							<textbox id="localidad" />
						</row>
						<row>
							<label value="Teléfono:" />
							<textbox id="telefono"/>
						</row>
						<row>
							<label value="Tipo de Empleado:" />
							<combobox ><comboitem label="DNI"/><comboitem label="LE"/></combobox>
						</row>						
					</rows>
				</grid>
			</center>
			<east>
				<vbox align="center">
					<button label="Aceptar" onClick="" />
					<button label="Cancelar" onClick="winAltaCliente.detach()" />
				</vbox>
			</east>
		</borderlayout>

	</window>
</zk>
